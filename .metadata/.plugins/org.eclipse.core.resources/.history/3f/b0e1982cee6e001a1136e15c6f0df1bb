package Bike;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike ducati =new MotorBike();
		MotorBike Honda =new MotorBike();
		ducati.start();
		Honda.start();
		ducati.gearchange();
		Honda.gearchange();
		ducati.stop();
		Honda.stop();
		ducati.setSpeed(100);
		Honda.setSpeed(80);
		
		ducati.setSpeed(70);
		Honda.setSpeed(50);


	}

}

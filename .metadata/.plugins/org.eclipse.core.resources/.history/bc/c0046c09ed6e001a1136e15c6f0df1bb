package FolderOfBooks;

public class Book {
	private int noOfCopies;
	void setnoOfCopies(int noOfCopies) {
		this.noOfCopies= noOfCopies;
		
		
		
	}
void openBook() {
	System.out.println("book opened");
}
void pageno() {
	System.out.println("page no 151");
}
void closebook() {
	System.out.println("book closed");
}
}

package Bike;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike ducati =new MotorBike();
		MotorBike Honda =new MotorBike();
		ducati.start();
		Honda.start();
		ducati.gearchange();
		Honda.gearchange();
		ducati.stop();
		Honda.stop();
		ducati.speed =100;
		Honda.speed=80;
		
		ducati.speed=70;
		Honda.speed=50;


	}

}
